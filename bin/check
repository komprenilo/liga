#!/bin/bash

if [[ "${SPARK_VERSION}" == "3.2.1" ]];then
  ./pants check python/liga/sklearn:sources@resolve=spark_3_2_1
elif [[ "${SPARK_VERSION}" == "3.3.1" ]];then
  ./pants check python/liga/sklearn:sources@resolve=spark_3_3_1
else
  ./pants check python/liga/sklearn:sources@resolve=spark_3_2_1
fi

